&FLUX_CONTROL
	INTG    = 0      ! Flag for interpolating gFiles
	NPSI    = 512	 ! Number of points in PsiN grid
	PACK    = 2.      ! Packing index for PsiN grid
	NTHETA  = 513    ! Number of points in theta grid (must be odd)
	NNC     = 20     ! Number of neoclassical harmonics
	H0      = 1.e-6  ! Initial integration step-length
	ACC     = 1.e-14 ! Integration accuracy
	ETA     = 1.e-8  ! Regularization factor for Green's function
	DR      = 1.e-2  ! Discritization parameter for simulated Mirnov data
	NTOR    = 2      ! Toroidal mode number
	MMIN    = 3	 ! Minimum poloidal mode number
	MMAX    = 30     ! Maximum toroidal mode number
	PSILIM  = 0.998  ! Ignore rational surfaces for PsiN > PSILIM
	TIME    = 0.     ! Experimental time
&end
